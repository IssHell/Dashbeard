<p id="notice"><%= notice %></p>

<h1>Listing Settings</h1>

<table>
  <thead>
    <tr>
      <th>Rssfeed</th>
      <th>Stocksymbol</th>
      <th>Weathercode</th>
      <th>Trainstationcode</th>
      <th>Busstationcode</th>
      <th>Refreshfrequency</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @settings.each do |setting| %>
      <tr>
        <td><%= setting.rssfeed %></td>
        <td><%= setting.stocksymbol %></td>
        <td><%= setting.weathercode %></td>
        <td><%= setting.trainstationcode %></td>
        <td><%= setting.busstationcode %></td>
        <td><%= setting.refreshfrequency %></td>
        <td><%= link_to 'Show', setting %></td>
        <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
        <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting', new_setting_path %>
